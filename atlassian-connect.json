    {
      "key": "zeplin-embed",
      "name": "Zeplin Embed",
      "description": "Embed Zeplin files into Confluence",
      "vendor": {
        "name": "Adam Thompson",
        "url": "https://www.adamthompson.ca"
      },
      "baseUrl": "{{localBaseUrl}}",
      "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}/atlassian-connect.json"
      },
      "authentication": {
        "type": "jwt"
      },
      "lifecycle": {
        "installed": "/installed"
      },
      "scopes": [
        "READ"
      ],
      "modules": {
        "dynamicContentMacros": [
          {
            "url": "/macro?projectID={projectID}&screenID={screenID}",
            "key": "zeplin-embed",
            "name": {
              "value": "Zeplin Embed"
            },
            "description": {
              "value": "Embeds a Zeplin image into Confluence"
            },
            "outputType": "block",
            "bodyType": "none",
            "parameters": [
              {
                "identifier": "projectID",
                "name": {
                  "value": "Project ID"
                },
                "type": "string",
                "required": true,
                "multiple": false
              },
              {
                "identifier": "screenID",
                "name": {
                  "value": "Screen ID"
                },
                "type": "string",
                "required": true,
                "multiple": false
              }
            ]
          }
        ]
      }
    }







